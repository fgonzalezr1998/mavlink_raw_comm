{
    "env": {
      "myDefaultIncludePath": [
          "${workspaceFolder}",
          "${workspaceFolder}/mavlink_raw_comm/include",
					"${workspaceFolder}/mavlink_raw_comm/tests/*"
				],
      "myCompilerPath": "/usr/bin/gcc"
    },
    "configurations": [
      {
        "name": "cpp_intellisense",
        "intelliSenseMode": "clang-x64",
        "includePath": [
            "${myDefaultIncludePath}", 
            "/usr/include/*",
            "/usr/lib/gcc/x86_64-linux-gnu/9/*"],
        "compilerPath": "/usr/bin/clang",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "browse": {
          "path": ["${workspaceFolder}"],
          "limitSymbolsToIncludedHeaders": true,
          "databaseFilename": ""
        }
      }
    ],
    "version": 4
  }